if skill 'Camping' < 100 
  if not findtype "kindling" backpack
      overhead "Materiale insufficiente!" 34
      wait 5000
      replay 
  elseif not findtype "campfire" true
      dclicktype "kindling" backpack
      wait 2000
  endif
else 
  overhead "Camping Ã¨ completa!" 20  
endif

loop
